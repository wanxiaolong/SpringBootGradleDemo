<?xml version="1.0" encoding="UTF-8" ?>
<FindBugsFilter>
    <!-- 匹配一个指定的类 -->
    <Match>
        <Class name="com.demo.gradle.Application"/>
    </Match>

    <!-- 匹配一个指定的包 -->
    <Match>
        <Package name="com.demo.gradle"/>
    </Match>

    <!-- 匹配一个固定的bug类型 -->
    <Match>
        <Bug pattern="DLS_DEAD_LOCAL_STORE" />
    </Match>
</FindBugsFilter>